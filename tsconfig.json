{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "src/*"
      ],
      "@/components": [
        "src/components/*"
      ],
      "@/consts": [
        "src/consts/*"
      ],
      "@/sections": [
        "src/sections/*"
      ],
      "@/icons": [
        "src/assets/icons/*"
      ],
      "@/skills": [
        "src/assets/icons/skills/*"
      ],
      "@/projects": [
        "src/assets/projects/*"
      ],
      "@/styles": [
        "src/styles/*"
      ]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}