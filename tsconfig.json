{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "src/*"
      ],
      "@/components": [
        "src/components/*"
      ],
      "@/icons": [
        "src/icons/*"
      ],
      "@/skills": [
        "src/icons/skills/*"
      ],
      "@/projects": [
        "src/projects/*"
      ],
      "@/styles": [
        "src/styles/*"
      ]
    }
  }
}